# Environment template for docling_preprocessor_factory
# Copy this file to .env and adjust values for your machine.

DOC_PREPROCESS_REQUIRED_PYTHON=3.12
DOC_PREPROCESS_PYTHON_BIN=python3.12

DOC_PREPROCESS_PROJECT_DIR=/absolute/path/to/docling_preprocessor_factory
DOC_PREPROCESS_CODE_DIR=${DOC_PREPROCESS_PROJECT_DIR}/code
DOC_PREPROCESS_SCRIPT_DIR=${DOC_PREPROCESS_CODE_DIR}/scripts

DOC_PREPROCESS_EXAMPLES_DIR=${DOC_PREPROCESS_CODE_DIR}/examples
DOC_PREPROCESS_OUTPUT_DIR=${DOC_PREPROCESS_CODE_DIR}/output
DOC_PREPROCESS_OUTPUT_JSONL=${DOC_PREPROCESS_OUTPUT_DIR}/preprocessed.jsonl

DOC_PREPROCESS_VENV_DIR=${DOC_PREPROCESS_CODE_DIR}/.venv
DOC_PREPROCESS_REQUIREMENTS_FILE=${DOC_PREPROCESS_SCRIPT_DIR}/requirements.txt
DOC_PREPROCESS_APP_FILE=${DOC_PREPROCESS_CODE_DIR}/preprocess_app.py
DOC_PREPROCESS_VERIFY_FILE=${DOC_PREPROCESS_CODE_DIR}/verify_output.py

DOC_PREPROCESS_LIBREOFFICE_BIN=
DOC_PREPROCESS_TESSERACT_BIN=tesseract

DOC_PREPROCESS_SUPPORTED_EXTENSIONS=.pdf,.docx,.xlsx,.xls,.pptx,.ppt
DOC_PREPROCESS_REQUIRED_FIELDS=source_file_path,input_format,unit_number,unit_type,text_markdown,ocr_image_text,metadata

DOC_PREPROCESS_SAMPLE_IMAGE_NAME=sample_ocr_image.png
DOC_PREPROCESS_SAMPLE_PDF_NAME=sample.pdf
DOC_PREPROCESS_SAMPLE_DOCX_NAME=sample.docx
DOC_PREPROCESS_SAMPLE_XLSX_NAME=sample.xlsx
DOC_PREPROCESS_SAMPLE_XLS_NAME=sample.xls
DOC_PREPROCESS_SAMPLE_PPTX_NAME=sample.pptx
DOC_PREPROCESS_SAMPLE_PPT_NAME=sample.ppt
